// Basic Syntax (Created by aaro_1 Just adding it incase I need it for later)
infectionclass = "Class-D"
Killer.Class ~= infectionclass :: jump(999)
wait(0.3)
SetClass(Target,infectionclass)
wait(0.1)
Teleport(Target,Killer.Position)

// Script - Testing
killerclass = killer
if killerclass == "Class-D" then
    SetClass(plr, "Class-D")
    wait(0.5)
    Teleport(Target,Killer.Position)                                      
end
if killerclass == "SCP-049-2" then
    SetClass(plr, "SCP-049-2")
    wait(0.5)
    Teleport(Target,Killer.Position)
end

// Testing V2

infectionclass = Killer.Class
Killer.Class ~= infectionclass :: jump(999)
wait(0.3)
SetClass(Target,infectionclass)
wait(0.1)
Teleport(Target,Killer.Position)
